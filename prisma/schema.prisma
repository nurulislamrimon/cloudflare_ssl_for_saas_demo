generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model CustomDomain {
  id       Int    @id @default(autoincrement())
  hostname String @unique
  status   String @default("pending")

  txt_name  String?
  txt_value String?
  method    String?

  ssl_status String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
